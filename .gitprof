export GIT_CONFIG=$HOMEBASE/.gitconfig

alias git10="git log | head"
alias git25="git log | head -n 25"
alias gitlm="git log | more"
alias gitss="git status ; git log | head -n 3"
alias gitcm="git checkout master"
alias gitco="git checkout"
alias gitdf="git diff"
alias gitdc="git diff --cached"
alias gitme="git log | grep -A4 -B1 Houhou | more"
alias gitmf="git merge --ff-only"

alias gitsv="git stash save"
alias gitli="git stash list"
alias gitap="git stash apply"
alias gitpop="git stash pop"

